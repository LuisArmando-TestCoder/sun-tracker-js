{
    "compilerOptions": {
      "target": "ES2020",
      "module": "ES2020",
      "lib": ["DOM", "ES2020"],
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "declaration": true,
      "declarationMap": true,
      "sourceMap": true
    },
    "include": ["src"]
  }
  